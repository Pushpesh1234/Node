<%- include('partials/head') %>
</head>
<body>
    
    <%- include('./partials/header')%>
    <header class="border-solid border-1 mt-5 border-gray-300 bg-[#FF5A5F] shadow-2xl text-white rounded-2xl pl-4 pt-8 pb-8 w-[50%] m-auto">
        <h1 class="text-center text-[50px] font-bold">
         Welcome to AirBnB
        </h1>
    </header>
    <section>
        <h1 class="text-center mt-10 font-bold bg-[#FF5A5F] text-white">
           Here are Registred Homes:
        </h1>
      
        <div class="flex">
        <ul class="ml-5 mt-[10px]">
         <% lonenly.forEach(homes=>{%>
    
          
            <img class="w-[300px] border border-1 border-[#FF5A5F] rounded-md" src="https://www.sarasotahomes.com/wp-content/uploads/2022/07/sarasota-fl-modern-homes-1.jpg">
           
            <div class="bg-[#FF5A5F] w-[300px] border-[#FF5A5F] rounded-md">
            <li class="text-[20px] text-white pl-4 font-bold "><%=homes.home%></li>
            <li class="text-[20px] text-white pl-4 font-bold "><%=homes.email%></li> 
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.price%></li>   
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.location%></li>  
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.rating%></li>
           </div>
                
              
            <%})%>
            
        </ul>
        </div>
        
       
    </section>
</body>
</html>