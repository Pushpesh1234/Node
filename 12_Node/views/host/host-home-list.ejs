<%- include("../partials/head")%>
</head>

<body>
<%- include("../partials/header")%>

</body>
<section>

    <h1 class="text-center text-[#FF5A5F] text-4xl font-bold pt-16">Host Registerd Homes</h1>
   <ul class="ml-5 mt-[10px] flex gap-10">
         <% lonenly.forEach(homes=>{%>

            <div class="bg-[#FF5A5F] w-[300px] border-[#FF5A5F] rounded-md">
           <img class="w-[300px]  border-1 border-[#FF5A5F] rounded-2xl" src="https://www.sarasotahomes.com/wp-content/uploads/2022/07/sarasota-fl-modern-homes-1.jpg">   
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.home%></li>
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.email%></li> 
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.price%></li>   
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.location%></li>  
            <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.rating%></li>
            
            <span class=" flex  justify-between py-2 px-2 "> 
              <button class="bg-[#6667AB] text-white cursor-pointer hover:bg-blue-300 transition-duration-300 rounded-sm  px-1">Delete</button>
            <button class="bg-green-500 rounded-sm text-white hover:bg-green-300 transition-duration-300 cursor-pointer px-1">Edit</button>
            </span>
             
           </div>
                
              
            <%})%>
            
        </ul>
</section>
</body>
</html>