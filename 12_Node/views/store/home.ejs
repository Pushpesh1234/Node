<%- include('../partials/head')%>
</head>
<body>
    
    <%- include('../partials/header')%>
    <header class="border-solid border-1 mt-5 border-gray-300 bg-[#FF5A5F] shadow-2xl text-white rounded-2xl pl-4 pt-8 pb-8 w-[50%] m-auto">
        <h1 class="text-center text-[50px] font-bold">
         Welcome to AirBnB
        </h1>
    </header>
    <section>
        <h1 class="text-center mt-10 font-bold bg-[#FF5A5F] text-white">
           Here are Registred Homes:
        </h1>
       
       <ul class=" py-15 px-10 flex gap-[40px] flex-wrap">
         <% lonenly.forEach(homes=>{%>

            <div class="bg-[#FF5A5F] border-1 border-[#FF5A5F] w-[250px] mx-4 rounded-md">
            <img class="w-[250px] h-[160px]   rounded-sm" src=<%=`${homes.image}`%>>   
            <li class="text-[20px] text-white pl-4 py-2 font-bold "> <%=homes.home%></li>
            <li class="text-[15px] text-white pl-4 py-0.5"> <%=homes.location%></li>   
            <li class="text-[15px] text-white pl-4 py-0.5"> $<%=homes.price%></li>   
            <!-- <li class="text-[20px] text-white pl-4 font-bold "> <%=homes.email%></li> -->
             <span class="flex justify-between pl-4 py-2">
                <img class="w-[30px] " src="https://cdn.pixabay.com/photo/2013/07/12/17/39/star-152151_1280.png">
                <li class="text-[20px] px-4 text-white pl-4 font-bold "><%=homes.rating%></li>
            </span>
           
             <span class="flex justify-between px-2 py-1 ">
                <button class="bg-red-500 cursor-pointer text-white px-1 rounded-sm  hover:bg-red-300 transition duration-300" >Book</button>
                <button class="bg-red-500 cursor-pointer text-white px-1 rounded-sm  hover:bg-red-300 transition duration-300" >Favourite</button>
            </span>
           </div>
                
              
            <%})%>
            
       </ul>
            

    </section>
</body>
</html>